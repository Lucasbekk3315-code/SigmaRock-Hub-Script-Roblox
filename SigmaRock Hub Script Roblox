local placeId = game.PlaceId

-- Lista de PlaceIds correspondientes a Blox Fruits y sus diferentes seas
local bloxFruitsSeas = {
    [2753915549] = true, -- First Sea
    [4442272183] = true, -- Second Sea
    [7449423635] = true, -- Third Sea
}

-- Verifica si el PlaceId actual pertenece a Blox Fruits
if bloxFruitsSeas[placeId] then
    print("Blox Fruits Sea detected")
loadstring(game:HttpGet("https://raw.githubusercontent.com/Lucasbekk3315-code/blox-fruits-2.2-roblox-script/refs/heads/main/Blox%20Fruits%20Script%20roblox%202.0%20BETA"))()
else
    print("PlaceId not recognized")
end
